  "license": "MIT",

  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-catch": "^1.0.6"
  },
  "dependencies": {
    "keycode": "^2.1.8"
  },
  "scripts": {
    "lint": "standard './*.js' './src/**/*.js'",
    "fix": "standard --fix './*.js' './src/**/*.js'",
